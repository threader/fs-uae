    if c.slow_memory:
        value = str(int(c.slow_memory) * 1024)
    else:
        value = "0"

    if value == "0":
        pass
    elif value == "524288":
        pass
    elif value == "1048576":
        pass
    elif value == "1572864":
        pass
    elif value == "1835008":
        pass
    else:
        f.warning("Unsupported slow memory size: " + value)
