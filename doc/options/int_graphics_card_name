Code:
    t = c.uae_gfxcard_type
    if t == "PicassoII":
        value = "Picasso II"
    elif t == "PicassoII+":
        value = "Picasso II+"
    elif t == "PicassoIV_Z2":
        value = "Picasso IV"
    elif t == "PicassoIV_Z3":
        value = "Picasso IV"
    elif t == "ZorroII":
        value = "UAEGFX"
    elif t == "ZorroIII":
        value = "UAEGFX"
    else:
        value = "Unknown GFX Card"